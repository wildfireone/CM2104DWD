<!-- views/pages/about.ejs -->

<!DOCTYPE html>
<html lang="en">

<head>
  <% include ../partials/head %>
</head>

<body class="container">

  <header>
    <% include ../partials/header %>
  </header>

  <main>
    <div class="row">
      <div class="col-sm-8">
        <div class="jumbotron">
          <h1><%= user.name %></h1>
          <h2><%= user.username %></h2>
          <p>
            <%= user.info %>
          </p>
        </div>
      </div>

      <div class="col-sm-4">
        <div class="well">
          <h3>Details</h3>
          <img src="<%= student.img %>" />
          <p>
            <%= user.location %>
          </p>
          <p>
            <%= user.age %>
          </p>

        </div>
      </div>

    </div>
  </main>

  <footer>
    <% include ../partials/footer %>
  </footer>

</body>

</html>
